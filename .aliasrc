alias unity_src='cd /opt/UnitySrc'
alias us='unity_src'

alias unity_build_editor_x64_debug='./jam MacEditor -sCONFIG=debug'
alias unity_build_editor_x64_release='./jam MacEditor -sCONFIG=release'
alias unity_build_editor_x64_debug_nolump='./jam MacEditor -sCONFIG=debug -sLUMP=0'
alias unity_build_editor_x64_release_nolump='./jam MacEditor -sCONFIG=release -sLUMP=0'
alias unity_build_editor_arm64_debug='./jam MacEditorArm64 -sCONFIG=debug'
alias unity_build_editor_arm64_release='./jam MacEditorArm64 -sCONFIG=release'
alias unity_build_editor_arm64_debug_nolump='./jam MacEditorArm64 -sCONFIG=debug -sLUMP=0'
alias unity_build_editor_arm64_release_nolump='./jam MacEditorArm64 -sCONFIG=release -sLUMP=0'
alias ubx='unity_build_editor_x64_debug'
alias ubxr='unity_build_editor_x64_release'
alias uba='unity_build_editor_arm64_debug'
alias ubar='unity_build_editor_arm64_release'

alias unity_run_editor_x64_debug='./build/MacEditor/x64/Debug/Unity.app/Contents/MacOS/Unity'
alias unity_run_editor_x64_release='./build/MacEditor/x64/Release/Unity.app/Contents/MacOS/Unity'
alias unity_run_editor_arm64_debug='./build/MacEditor/arm64/Debug/Unity.app/Contents/MacOS/Unity'
alias unity_run_editor_arm64_release='./build/MacEditor/arm64/Release/Unity.app/Contents/MacOS/Unity'
alias urx='unity_run_editor_x64_debug'
alias urxr='unity_run_editor_x64_release'
alias ura='unity_run_editor_arm64_debug'
alias urar='unity_run_editor_arm64_release'

alias unity_debug_editor_x64_debug='lldb ./build/MacEditor/x64/Debug/Unity.app/Contents/MacOS/Unity'
alias unity_debug_editor_x64_release='lldb ./build/MacEditor/x64/Release/Unity.app/Contents/MacOS/Unity'
alias unity_debug_editor_arm64_debug='lldb ./build/MacEditor/arm64/Debug/Unity.app/Contents/MacOS/Unity'
alias unity_debug_editor_arm64_release='lldb ./build/MacEditor/arm64/Release/Unity.app/Contents/MacOS/Unity'
alias udx='unity_debug_editor_x64_debug'
alias udxr='unity_debug_editor_x64_release'
alias uda='unity_debug_editor_arm64_debug'
alias udar='unity_debug_editor_arm64_release'

alias unity_utr_native='perl utr.pl --suite=native'
alias unity_utr_editor='perl utr.pl --suite=editor'
alias unity_utr_integration='perl utr.pl --suite=integration'
alias unity_utr_native_cacheserver='perl utr.pl --suite=native --testfilter=AssetDatabase_'
alias unity_utr_editor_cacheserver='perl utr.pl --suite=editor --testproject=CacheServer'
alias unity_utr_integration_cacheserver='perl utr.pl --suite=integration --testfilter=Unity.IntegrationTests.AssetImporting.CacheServerV2'

alias unity_xcode='open ./Projects/Xcode/MacEditor.xcodeproj'
alias unity_projectfiles='./jam ProjectFiles'
alias ux='unity_xcode'
alias up='unity_projectfiles'

alias -g unity_editor_log='~/Library/Logs/Unity/Editor.log'
alias -g unity_editor_log_prev='~/Library/Logs/Unity/Editor-prev.log'
alias unity_editor_log_less='less ~/Library/Logs/Unity/Editor.log'
alias unity_editor_log_follow='less --follow-name +F ~/Library/Logs/Unity/Editor.log'
alias -g ul='unity_editor_log'
alias -g ulp='unity_editor_log_prev'
alias ull='unity_editor_log_less'
alias ulf='unity_editor_log_follow'

alias unity_binary2text='./build/MacEditor/arm64/Debug/Unity.app/Contents/Tools/binary2text'

